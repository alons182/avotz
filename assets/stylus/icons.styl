		url('../fonts/avotz.ttf?oryxob') format('truetype'),
		url('../fonts/avotz.woff?oryxob') format('woff'),
		url('../fonts/avotz.svg?oryxob#avotz') format('svg');
	font-weight: normal;
	font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
